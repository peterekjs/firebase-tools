{
  "name": "@peterek/firebase-tools",
  "version": "0.1.1",
  "description": "Set of handy tools for Firebase based projects",
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "firebase",
    "rxjs"
  ],
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/firebase-tools.git"
  },
  "bugs": {
    "url": "https://github.com/peterekjs/firebase-tools/issues"
  },
  "homepage": "https://github.com/peterekjs/firebase-tools#readme",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/firebase-tools.cjs",
  "module": "./dist/firebase-tools.js",
  "exports": {
    ".": {
      "import": "./dist/firebase-tools.js",
      "require": "./dist/firebase-tools.cjs"
    },
    "./src/*": "./src/*"
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@peterek/helpers": "^1.0.2",
    "rxfire": "^6.0.3"
  },
  "peerDependencies": {
    "@firebase/firestore": "^3.10.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@peterek/rollup-externals": "^1.0.0",
    "@types/ramda": "^0.28.23",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0"
  }
}
